{
  "name": "cesnet-site_hadoop",
  "version": "2.1.0",
  "author": "Frantisek Dvorak <valtri@civ.zcu.cz>",
  "summary": "Hadoop Site-Specific Module",
  "license": "MIT",
  "source": "https://github.com/MetaCenterCloudPuppet/cesnet-site_hadoop",
  "project_page": "https://github.com/MetaCenterCloudPuppet/cesnet-site_hadoop",
  "issues_url": "https://github.com/MetaCenterCloudPuppet/cesnet-site_hadoop/issues",
  "tags": [
    "hadoop",
    "accounting",
    "cloudera"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": ["6", "7"]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": ["7", "8"]
    },
    {
      "operatingsystem": "Fedora"
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["6", "7"]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["14.04"]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": ["6", "7"]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.4.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.4.0"
    }
  ],
  "dependencies": [
    {
      "name": "cesnet/hadoop",
      "version_requirement": ">= 2.4.0 < 3.0.0"
    },
    {
      "name": "cesnet/hbase",
      "version_requirement": "< 1.0.0"
    },
    {
      "name": "cesnet/hive",
      "version_requirement": ">= 0.11.0 < 1.0.0"
    },
    {
      "name": "cesnet/java_ng",
      "version_requirement": ">= 2.1.0 < 3.0.0"
    },
    {
      "name": "cesnet/oozie",
      "version_requirement": ">= 0.10.0 < 1.0.0"
    },
    {
      "name": "cesnet/pig",
      "version_requirement": "< 1.0.0"
    },
    {
      "name": "cesnet/spark",
      "version_requirement": ">= 1.0.0 < 2.0.0"
    },
    {
      "name": "cesnet/zookeeper",
      "version_requirement": "< 1.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": "< 3.0.0"
    },
    {
      "name": "puppetlabs/mysql",
      "version_requirement": "< 4.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 1.0.0 < 5.0.0"
    }
  ]
}
